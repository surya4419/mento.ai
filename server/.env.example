# Server Environment Variables Template
# Copy this file to .env and update with your actual values

# Server Configuration
NODE_ENV=development
PORT=3001

# Tavus API Configuration
TAVUS_API_KEY=your_real_tavus_api_key_here
TAVUS_API_URL=https://tavusapi.com/v2
TAVUS_REPLICA_ID=your_replica_id_here
TAVUS_DEFAULT_PERSONA_ID=your_persona_id_here  # Optional

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX=100

# Security
# SESSION_SECRET=your_session_secret_here  # Generate a strong secret for production

# Logging
LOG_LEVEL=debug